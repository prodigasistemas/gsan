<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="EfetuarSupressaoLigacaoAguaActionForm"
			type="gcom.gui.atendimentopublico.EfetuarSupressaoLigacaoAguaActionForm" />
	</form-beans>
	<!--******************* PÃGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>

		<action name="EfetuarSupressaoLigacaoAguaActionForm"
			type="gcom.gui.atendimentopublico.ExibirEfetuarSupressaoLigacaoAguaAction"
			validate="true" scope="session"
			path="/exibirEfetuarSupressaoLigacaoAguaAction">
			<forward name="efetuarSupressaoLigacaoAgua"
				path="/jsp/atendimentopublico/ligacaoagua/ligacao_agua_supressao_efetuar.jsp" />
		</action>

		<action name="EfetuarSupressaoLigacaoAguaActionForm"
			type="gcom.gui.atendimentopublico.EfetuarSupressaoLigacaoAguaAction"
			validate="true" scope="session"
			path="/efetuarSupressaoLigacaoAguaAction">
			
			<forward name="encerrarOrdemServicoAction"
				path="/encerrarOrdemServicoAction.do" />
				
			<forward name="encerrarOrdemServicoPopupAction"
				path="/encerrarOrdemServicoPopupAction.do" />
		</action>

	</action-mappings>
	<message-resources null="false"
		parameter="gcom.properties.application" />
</struts-config>