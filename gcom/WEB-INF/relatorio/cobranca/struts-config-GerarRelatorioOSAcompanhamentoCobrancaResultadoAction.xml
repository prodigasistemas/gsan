			type="gcom.gui.relatorio.cobranca.ExibirGerarRelatorioOSAcompanhamentoCobrancaResultadoAction"
			validate="false" 
			scope="session"
			path="/exibirGerarRelatorioOSAcompanhamentoCobrancaResultadoAction">
