<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>
	<form-beans>
		<form-bean name="AtualizarFaturamentoMovimentoCelularActionForm"
			type="gcom.gui.faturamento.AtualizarFaturamentoMovimentoCelularActionForm" />


	</form-beans>

	<!--******************* PÃGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>

		<action  
			type="gcom.gui.faturamento.ExibirAtualizarFaturamentoMovimentoCelularAction" 
			scope="session"
			path="/exibirAtualizarFaturamentoMovimentoCelularAction">

			<forward name="atualizarFaturamentoMovimentoCelular"
			path="/jsp/faturamento/atualizar_faturamento_movimento_celular.jsp" />

		</action>

		<action 
			type="gcom.gui.faturamento.AtualizarFaturamentoMovimentoCelularAction"
			path="/atualizarFaturamentoMovimentoCelularAction">
		</action>
	</action-mappings>
</struts-config>