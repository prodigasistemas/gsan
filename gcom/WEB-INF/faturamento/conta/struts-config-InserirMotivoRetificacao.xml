		<action name="InserirMotivoRetificacaoActionForm"
			type="gcom.gui.faturamento.conta.InserirMotivoRetificacaoAction" scope="session"
			validate="true" path="/inserirMotivoRetificacaoAction" />
			
			</action-mappings>

	<message-resources null="false"
