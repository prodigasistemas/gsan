<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>
		<form-bean
			name="ExibirCalendarioAbastecimentoPortalCompesaActionForm"
			type="gcom.gui.portal.ExibirCalendarioAbastecimentoPortalCompesaActionForm" />
	</form-beans>

	<!--******************* PÃGINAS GLOBAIS *******************-->
	<global-forwards />
	<action-mappings>

		<action
			name="ExibirCalendarioAbastecimentoPortalCompesaActionForm"
			type="gcom.gui.portal.ExibirCalendarioAbastecimentoPortalCompesaAction"
			validate="false" scope="session"
			path="/exibirCalendarioAbastecimentoPortalCompesaAction">

			<forward
				name="exibirCalendarioAbastecimentoPortalCompesaAction"
				path="/jsp/portal/calendario_abastecimento_home.jsp" />


		</action>
	</action-mappings>

	<message-resources null="false"
		parameter="gcom.properties.application" />
</struts-config>

