<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts-config PUBLIC "-//Apache Software Foundation//DTD Struts Configuration 1.1//EN" "http://jakarta.apache.org/struts/dtds/struts-config_1_1.dtd">
<struts-config>

	<form-beans>
		<form-bean name="AlterarLeituristaArquivoLeituraActionForm"
			type="gcom.gui.micromedicao.AlterarLeituristaArquivoLeituraActionForm" />
			
	</form-beans>

	<!--******************* PÃGINAS GLOBAIS *******************-->
	<global-forwards />

	<action-mappings>
		<action name="AlterarLeituristaArquivoLeituraActionForm"
			type="gcom.gui.micromedicao.ExibirAlterarLeituristaArquivoLeituraAction"
			validate="true" scope="session"
			path="/exibirAlterarLeituristaArquivoLeituraAction">

			<forward name="AlterarLeituristaArquivoLeitura"
				path="/jsp/micromedicao/alterar_leiturista_arquivo_leitura.jsp" />
		</action>

		<action name="AlterarLeituristaArquivoLeituraActionForm"
			type="gcom.gui.micromedicao.AlterarLeituristaArquivoLeituraAction"
			validate="true" scope="session"
			path="/alterarLeituristaArquivoLeituraAction">
			
		</action>
			
		

	</action-mappings>

	<message-resources null="false"
		parameter="gcom.properties.application" />

</struts-config>